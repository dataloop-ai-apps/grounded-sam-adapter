{
  "name": "grounded-sam2",
  "displayName": "Grounded SAM2",
  "version": "0.1.21",
  "scope": "public",
  "description": "a very interesting demo by combining Grounding DINO and Segment Anything which aims to detect and segment anything with text inputs! And we will continue to improve it and create more interesting demos based on this foundation.",
  "attributes": {
    "Provider": "Other",
    "Category": "Model",
    "Computer Vision": "Object Detection",
    "License": "Apache 2.0",
    "Media Type": [
      "Image"
    ],
    "Deployed By": "Dataloop"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/grounded-sam-adapter.git",
    "gitTag": "0.1.21"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "grounded-sam2-deploy",
        "runtime": {
          "podType": "regular-m",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/grounded-sam-adapter:0.1.4",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "grounded-sam2-module",
        "entryPoint": "adapters/grounded_sam2_adapter/grounded_sam2_adapter.py",
        "className": "GroundedSam2",
        "computeConfig": "grounded-sam2-deploy",
        "description": "GroundedSam2 Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate GroundedSam2 model performance"
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "GroundedSam2 predict function"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": ""
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
            ],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Function to run GroundedSam2 inference on a dataset"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "grounded-sam2",
        "moduleName": "grounded-sam2-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "recipe_id": "",
          "box_threshold": 0.2,
          "text_threshold": 0.2,
          "nms_threshold": 0.8,
          "checkpoint_filepath": "artifacts/sam2_hiera_large.pt",
          "checkpoint_url": "https://storage.googleapis.com/model-mgmt-snapshots/sam2/sam2_hiera_large.pt"
        },
        "description": "Object detection with text prompts, segment using SAM2"
      }
    ]
  }
}