{
  "name": "sam2-adapter",
  "displayName": "SAM2",
  "version": "0.1.20",
  "scope": "public",
  "description": "The Segment Anything Model (SAM2) produces high quality object masks from input prompts such as points or boxes, and it can be used to generate masks for all objects in an image. It has been trained on a dataset of 11 million images and 1.1 billion masks, and has strong zero-shot performance on a variety of segmentation tasks.",
  "attributes": {
    "Provider": "Meta",
    "Category": "Model",
    "Computer Vision": "Semantic Segmentation",
    "License": "Apache 2.0",
    "Media Type": [
      "Image"
    ],
    "Deployed By": "Dataloop"
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/grounded-sam-adapter.git",
    "gitTag": "0.1.20"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "sam2-deploy",
        "runtime": {
          "podType": "regular-m",
          "concurrency": 1,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/grounded-sam-adapter:0.1.4",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "sam2-module",
        "entryPoint": "adapters/sam2_adapter/sam2_adapter.py",
        "className": "SegmentAnythingAdapter",
        "computeConfig": "sam2-deploy",
        "description": "Sam2 Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate Sam2 model performance"
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input images for prediction."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The predicted annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Sam2 predict function"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": ""
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "Dataloop Filter DQL"
              }
            ],
            "output": [
            ],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Function to run Sam inference on a dataset"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "sam2-model-adapter",
        "moduleName": "sam2-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "stability_score_threshold": 0.85,
          "predicted_iou_threshold": 0.85,
          "output_type": "polygon",
          "checkpoint_filepath": "artifacts/sam2_hiera_large.pt",
          "checkpoint_url": "https://storage.googleapis.com/model-mgmt-snapshots/sam2/sam2_hiera_large.pt",
          "id_to_label_map": {}
        },
        "description": "Segment Anything 2 model"
      }
    ]
  }
}
